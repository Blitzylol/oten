end
coroutine.wrap(LTCASSV_fake_script)()
local function SNIB_fake_script() -- amount.Script 
	local script = Instance.new('Script', amount)

	function comma_value(n) -- credit http://richard.warburton.it
		local left,num,right = string.match(n,'^([^%d]*%d)(%d*)(.-)$')
		return left..(num:reverse():gsub('(%d%d%d)','%1,'):reverse())..right
	end
    repeat wait() until game:GetService("Players").LocalPlayer
	local player = game:GetService("Players").LocalPlayer
    local cr = player.DataFolder.Currency.Value
	local amout = (comma_value(cr))
    
	while wait(0.2) do 
        local cr = player.DataFolder.Currency.Value
        local amout = (comma_value(cr))
		script.Parent.Text = amout
	end
end
coroutine.wrap(SNIB_fake_script)()
